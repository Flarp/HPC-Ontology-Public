#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( hpc:AddressOp
                hpc:IndirectionOp
                hpc:LogicalNegOp
                hpc:OnesComplementOp
                hpc:PostfixDecrementOp
                hpc:PostfixIncrementOp
                hpc:PrefixDecrementOp
                hpc:PrefixIncrementOp
                hpc:PrefixMinusOp
                hpc:PrefixPlusOp
              )
] .
