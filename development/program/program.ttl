hpc:Unsorted rdf:type owl:Class ;
             rdfs:subClassOf hpc:LanguageConstruct .

###  https://github.com/HPC-FAIR/HPC-Ontology#Value
hpc:Value rdf:type owl:Class ;
          rdfs:subClassOf hpc:Unsorted ;
          rdfs:comment "SgXXXVal = constant?" .


###  https://github.com/HPC-FAIR/HPC-Ontology#Variable
hpc:Variable rdf:type owl:Class ;
             rdfs:subClassOf hpc:Expression ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:declaredBy ;
                               owl:someValuesFrom hpc:Declaration
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:definedBy ;
                               owl:someValuesFrom hpc:Declaration
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasInitializer ;
                               owl:someValuesFrom hpc:Initializer
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasScope ;
                               owl:someValuesFrom hpc:Scope
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasSize ;
                               owl:someValuesFrom hpc:Literal
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasStorageClass ;
                               owl:someValuesFrom hpc:StorageClass
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasType ;
                               owl:someValuesFrom hpc:Type
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:name ;
                               owl:someValuesFrom hpc:Literal
                             ] ;
             rdfs:comment """synonyms: object
named location of storage""" .


###  https://github.com/HPC-FAIR/HPC-Ontology#VariableDecl
hpc:VariableDecl rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Declaration .


###  https://github.com/HPC-FAIR/HPC-Ontology#VariableDefinition
hpc:VariableDefinition rdf:type owl:Class ;
                       rdfs:subClassOf hpc:Declaration .


###  https://github.com/HPC-FAIR/HPC-Ontology#VariableRef
hpc:VariableRef rdf:type owl:Class ;
                rdfs:subClassOf hpc:PrimaryExpr ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty hpc:referTo ;
                                  owl:someValuesFrom hpc:Variable
                                ] .
###  https://github.com/HPC-FAIR/HPC-Ontology#VoidType
hpc:VoidType rdf:type owl:Class ;
             rdfs:subClassOf hpc:BasicType .


###  https://github.com/HPC-FAIR/HPC-Ontology#Volatile
hpc:Volatile rdf:type owl:Class ;
             rdfs:subClassOf hpc:TypeQualifier .


###  https://github.com/HPC-FAIR/HPC-Ontology#WCharType
hpc:WCharType rdf:type owl:Class ;
              rdfs:subClassOf hpc:CharType .
###  https://github.com/HPC-FAIR/HPC-Ontology#WhileStatement
hpc:WhileStatement rdf:type owl:Class ;
                   rdfs:subClassOf hpc:IterationStatement .


###  https://github.com/HPC-FAIR/HPC-Ontology#XorAssignOp
hpc:XorAssignOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:AssignmentOp .


###  https://github.com/HPC-FAIR/HPC-Ontology#XorOp
hpc:XorOp rdf:type owl:Class ;
          rdfs:subClassOf hpc:BinaryOp .


###  https://github.com/HPC-FAIR/HPC-Ontology#isAssignedTo
hpc:isAssignedTo rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Expression ,
                                 hpc:Unsorted .


