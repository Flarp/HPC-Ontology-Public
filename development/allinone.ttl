                               owl:someValuesFrom hpc:Scope
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasSize ;
                               owl:someValuesFrom hpc:Literal
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasStorageClass ;
                               owl:someValuesFrom hpc:StorageClass
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:hasType ;
                               owl:someValuesFrom hpc:Type
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hpc:name ;
                               owl:someValuesFrom hpc:Literal
                             ] ;
             rdfs:comment """synonyms: object
named location of storage""" .


