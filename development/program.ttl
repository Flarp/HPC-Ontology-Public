#################################################################
#    Annotation properties
#################################################################

#################################################################
#    Object Properties
#################################################################

hpc:access rdf:type owl:ObjectProperty ;
           owl:inverseOf hpc:accessedBy ;
           rdfs:comment "function relation" .


hpc:accessedBy rdf:type owl:ObjectProperty ;
               rdfs:comment "variable relation" .

hpc:allocate rdf:type owl:ObjectProperty ;
             owl:inverseOf hpc:allocatedBy .

hpc:alias rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:graphLink ;
            schema:domainIncludes hpc:Variable ;
            schema:rangeIncludes hpc:Variable ;
            rdfs:comment "Indicates a variable aliases another variable within some context" .



hpc:allocatedBy rdf:type owl:ObjectProperty ;
                rdfs:comment "array relation" .

hpc:upstreamCallSite rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf hpc:graphLink ;
                     owl:inverseOf hpc:destFunc ;
                     schema:domainIncludes hpc:Function ;
                     schema:rangeIncludes hpc:Callsite ;
                     rdfs:comment "Inverse of a call site --> function being called." .

hpc:callSite rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf hpc:graphLink ;
             owl:inverseOf hpc:srcFunc ;
             schema:domainIncludes hpc:Function ;
             schema:rangeIncludes hpc:Callsite ;
             rdfs:comment """a function has one or more call sites where it calls other functions.
This property links a function to its function call sites.""" .


hpc:calledBy rdf:type owl:ObjectProperty ;
             owl:equivalentProperty hpc:caller ;
             rdfs:subPropertyOf hpc:graphLink ;
             owl:inverseOf hpc:calls .


hpc:callee rdf:type owl:ObjectProperty ;
           owl:equivalentProperty hpc:calls ;
           rdfs:subPropertyOf hpc:graphLink ;
           owl:inverseOf hpc:caller .


hpc:caller rdf:type owl:ObjectProperty ;
           schema:rangeIncludes hpc:Function ;
           schema:domainIncludes hpc:Function ;
           rdfs:subPropertyOf hpc:graphLink .


hpc:calls rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf hpc:graphLink ;
          schema:rangeIncludes hpc:Function ;
          schema:domainIncludes hpc:Function ;
          rdfs:comment "a function calls another function" .


hpc:deallocatedBy rdf:type owl:ObjectProperty ;
                  rdfs:comment "array relation" .

hpc:declaredBy rdf:type owl:ObjectProperty ;
               schema:domainIncludes hpc:Variable ;
               schema:rangeIncludes hpc:Statement ;
               rdfs:comment "a variable is declared by a statment" ,
                            "variable relation" .


hpc:definedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf hpc:graphLink .


hpc:destFunc rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf hpc:graphLink ;
             owl:inverseOf hpc:upstreamCallSite ;
             schema:rangeIncludes hpc:Function ;
             rdfs:comment "linke a call site to the function being called." .

hpc:hasAlignment rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf hpc:graphLink ;
                 rdfs:comment "AST node to alignment node" .


hpc:hasArgumentExpr rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf hpc:graphLink .


hpc:hasArgumentList rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf hpc:graphLink .


hpc:hasArrayBaseType rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf hpc:hasType ;
                     rdfs:range hpc:Type .


hpc:hasArrayRank rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:comment "?" .


hpc:hasBaseType rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:hasType .


hpc:hasBody rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:graphLink ;
            rdfs:comment "AST has a body" .


hpc:hasChild rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf hpc:graphLink ;
             owl:inverseOf hpc:hasParent .


hpc:hasCondition rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf hpc:graphLink ;
                 rdfs:comment "AST node to its condition node" .


hpc:hasDeclaration rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf hpc:graphLink ;
                   rdfs:comment "Domain: definition" .


hpc:hasDefinition rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf hpc:graphLink .


hpc:hasElseBody rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:graphLink .


hpc:hasExpression rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf hpc:graphLink ;
                  rdfs:domain hpc:ExpressionStatement .


hpc:hasFalseBody rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf hpc:graphLink .


hpc:hasFile rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:graphLink ;
            rdfs:comment "AST node to its file node" .


hpc:hasForIncr rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf hpc:graphLink .


hpc:hasForInit rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf hpc:graphLink .


hpc:hasForTest rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf hpc:graphLink .


hpc:hasHiddenType rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf hpc:hasType .


hpc:hasIndexExp rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:graphLink .


hpc:hasInitStatement rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf hpc:graphLink .


hpc:hasInitialValue rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf hpc:graphLink .


hpc:hasInitializer rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf hpc:graphLink .

hpc:hasKey rdf:type owl:ObjectProperty ;
           rdfs:comment "case key:" .


hpc:hasLabelRef rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:graphLink .


hpc:hasLeftOperand rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf hpc:hasChild ;
                   rdfs:domain hpc:BinaryOp .

hpc:hasOperand rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf hpc:hasChild .

hpc:hasOperator rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:graphLink .

hpc:hasParameter rdf:type owl:ObjectProperty ;
                 schema:rangeIncludes hpc:Parameter ;
                 rdfs:subPropertyOf hpc:graphLink .


hpc:hasParameterDeclList rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf hpc:graphLink ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain hpc:FunctionType ;
                         rdfs:comment "function relation" .


hpc:hasParameterList rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf hpc:graphLink .


hpc:hasParent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf hpc:graphLink .


hpc:hasReturnType rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf hpc:hasType ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain hpc:FunctionType ;
                  rdfs:range hpc:Type ;
                  rdfs:comment "function relation" .


hpc:hasRightOperand rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf hpc:hasChild ;
                    rdfs:domain hpc:BinaryOp .

hpc:hasScope rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf hpc:graphLink .


hpc:hasSelector rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:graphLink .

hpc:hasStatement rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf hpc:graphLink .


hpc:hasStorageClass rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf hpc:graphLink ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:comment "variable relation" .


hpc:hasTarget rdf:type owl:ObjectProperty .


hpc:hasTrueBody rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hpc:graphLink .


hpc:hasType rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:graphLink ;
            rdf:type owl:FunctionalProperty ;
            rdfs:range hpc:Type ;
            rdfs:comment "variable relation" .


hpc:hasValue rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf hpc:graphLink .

hpc:read rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf hpc:access ;
         owl:inverseOf hpc:readBy .


hpc:readBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf hpc:accessedBy .


hpc:referTo rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:graphLink ;
            owl:inverseOf hpc:referredBy .


hpc:referredBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf hpc:graphLink .


hpc:relationOp rdf:type owl:ObjectProperty .

hpc:sourceFile rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf hpc:file .


hpc:srcFunc rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf hpc:graphLink ;
            schema:rangeIncludes hpc:Function ;
            schema:domainIncludes hpc:Callsite ;
            rdfs:comment "Inverse of Function--> CallSite" .

hpc:write rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf hpc:access ;
          owl:inverseOf hpc:writtenBy .


hpc:writtenBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf hpc:accessedBy .


hpc:writtenIn rdf:type owl:ObjectProperty ;
              rdfs:domain hpc:LanguageConstruct ;
              rdfs:range hpc:ProgrammingModel .


hpc:AssignInitializer rdf:type owl:Class ;
                      rdfs:subClassOf hpc:Initializer ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty hpc:hasInitialValue ;
                                        owl:someValuesFrom hpc:Expression
                                      ] .

#################################################################
#    Data properties
#################################################################

hpc:kernelName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf hpc:name .


hpc:hasValue rdf:type owl:DatatypeProperty .


hpc:Nth rdf:type owl:DatatypeProperty .


hpc:NthArg rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf hpc:Nth .


hpc:NthPara rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf hpc:Nth .



hpc:allocatedDataSize rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf hpc:size .


hpc:arrayID rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf hpc:id .


hpc:arrayName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf hpc:name .

hpc:beginMemoryAddress rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf hpc:memoryAddress .

hpc:codeVariant rdf:type owl:DatatypeProperty .


hpc:columnNumber rdf:type owl:DatatypeProperty ;
                 rdfs:comment "Column number within a file" .


hpc:commandLineOption rdf:type owl:DatatypeProperty .

hpc:endMemoryAddress rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:memoryAddress .

hpc:lineNumber rdf:type owl:DatatypeProperty ;
               rdfs:comment "line number within a file" .



#################################################################
#    Classes
#################################################################

hpc:RegisterStorage rdf:type owl:Class ;
                    rdfs:subClassOf hpc:StorageClass .


hpc:Kernel rdf:type owl:Class ;
           rdfs:subClassOf hpc:Statement ;
           rdfs:comment "A kernel is a function or subroutine of a program written in any programming languages. A kernel has a defined function body and can be executed on a computer. This is a generic concept used for performance analysis."@en .


hpc:LanguageConstruct rdf:type owl:Class .


hpc:SizeofOp rdf:type owl:Class ;
             rdfs:subClassOf hpc:UnaryOp .


hpc:MainFunction rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Function ;
                 rdfs:comment "The main function in C/C++. Or similar entry function in other programming languages." .


hpc:ClassDeclaration rdf:type owl:Class ;
                     rdfs:subClassOf hpc:Declaration .

hpc:CommaOp rdf:type owl:Class ;
            rdfs:subClassOf hpc:Expression .


hpc:AddAssignOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:AssignmentOp .


hpc:AddOp rdf:type owl:Class ;
          rdfs:subClassOf hpc:BinaryOp .


hpc:AdditiveOp rdf:type owl:Class ;
               rdfs:subClassOf hpc:Expression .


hpc:AddressOp rdf:type owl:Class ;
              rdfs:subClassOf hpc:UnaryOp ;
              rdfs:comment "&" .


hpc:AndAssignOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:AssignmentOp .


hpc:AndOp rdf:type owl:Class ;
          rdfs:subClassOf hpc:BinaryOp .


hpc:ArrayDecl rdf:type owl:Class ;
              rdfs:subClassOf hpc:Declaration .


hpc:ArraySub rdf:type owl:Class ;
             rdfs:subClassOf hpc:PostfixOp ;
             rdfs:comment "[]" .


hpc:ArrayType rdf:type owl:Class ;
              rdfs:subClassOf hpc:DerivedType .


hpc:ArrowExp rdf:type owl:Class ;
             rdfs:subClassOf hpc:MemberRef .


hpc:AssignOp rdf:type owl:Class ;
             rdfs:subClassOf hpc:AssignmentOp .


hpc:AssignStatement rdf:type owl:Class ;
                    rdfs:subClassOf hpc:Statement .


hpc:AssignmentOp rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Expression .


hpc:Auto rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( hpc:auto
                                         )
                             ] ;
         rdfs:subClassOf hpc:StorageClass .


hpc:BasicBlock rdf:type owl:Class ;
               rdfs:subClassOf hpc:ScopeStatement .


hpc:BasicType rdf:type owl:Class ;
              rdfs:subClassOf hpc:Type ;
              rdfs:comment "Because objects of these types can be interpreted as numbers, they will be referred to as arithmetic types." .



hpc:BinaryOp rdf:type owl:Class ;
             rdfs:subClassOf hpc:Expression ;
             rdfs:comment "overlap" .


hpc:BitwiseANDOp rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Expression .


hpc:BitwiseOROp rdf:type owl:Class ;
                rdfs:subClassOf hpc:Expression .


hpc:BitwiseShiftOp rdf:type owl:Class ;
                   rdfs:subClassOf hpc:Expression .


hpc:BitwiseXOROp rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Expression .


hpc:Block rdf:type owl:Class ;
          rdfs:subClassOf hpc:Scope .


hpc:BoolType rdf:type owl:Class ;
             rdfs:subClassOf hpc:BasicType .


hpc:BreakStatement rdf:type owl:Class ;
                   rdfs:subClassOf hpc:JumpStatement .


hpc:CallGraph rdf:type owl:Class ;
              rdfs:subClassOf hpc:Dataset .


hpc:CallSite rdf:type owl:Class ;
             owl:equivalentClass hpc:FunctionCall ;
             rdfs:subClassOf hpc:Expression .


hpc:CaseOptionStatement rdf:type owl:Class ;
                        rdfs:subClassOf hpc:Statement .


hpc:CastOp rdf:type owl:Class ;
           rdfs:subClassOf hpc:Expression .


hpc:CharConst rdf:type owl:Class ;
              rdfs:subClassOf hpc:Constant .


hpc:CharType rdf:type owl:Class ;
             rdfs:subClassOf hpc:BasicType .


hpc:ComplexType rdf:type owl:Class ;
                rdfs:subClassOf hpc:BasicType .


hpc:CompoundStatement rdf:type owl:Class ;
                      rdfs:subClassOf hpc:Statement ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty hpc:consistsOf ;
                                        owl:someValuesFrom hpc:Statement
                                      ] ;
                      rdfs:comment "The body of a function definition is a compound statement." .




hpc:ConditionalOp rdf:type owl:Class ;
                  rdfs:subClassOf hpc:Expression .


hpc:Const rdf:type owl:Class ;
          rdfs:subClassOf hpc:TypeQualifier .


hpc:Constant rdf:type owl:Class ;
             rdfs:subClassOf hpc:PrimaryExpr .


hpc:ContinueStatement rdf:type owl:Class ;
                      rdfs:subClassOf hpc:JumpStatement .



hpc:Declaration rdf:type owl:Class ;
                rdfs:subClassOf hpc:Statement ;
                rdfs:comment "Declarations specify the interpretation given to each identifier; they do not necessarily reserve storage associated with the identifier. Declarations that reserve storage are called definitions." .

hpc:IOStatement rdf:type owl:Class ;
                rdfs:subClassOf hpc:Statement .


hpc:IfElseStatement rdf:type owl:Class ;
                    rdfs:subClassOf hpc:SelectionStatement .


hpc:IfStatement rdf:type owl:Class ;
                rdfs:subClassOf hpc:SelectionStatement .


hpc:IndirectionOp rdf:type owl:Class ;
                  rdfs:subClassOf hpc:UnaryOp ;
                  rdfs:comment "*" .


hpc:Initializer rdf:type owl:Class ;
                rdfs:subClassOf hpc:Expression .


hpc:IntConst rdf:type owl:Class ;
             rdfs:subClassOf hpc:Constant .


hpc:IntType rdf:type owl:Class ;
            rdfs:subClassOf hpc:BasicType .

hpc:IterationStatement rdf:type owl:Class ;
                       owl:equivalentClass hpc:Loop ;
                       rdfs:subClassOf hpc:Statement .


hpc:JumpStatement rdf:type owl:Class ;
                  rdfs:subClassOf hpc:Statement .


hpc:LabeledStatement rdf:type owl:Class ;
                     rdfs:subClassOf hpc:Statement .


hpc:LessOp rdf:type owl:Class ;
           rdfs:subClassOf hpc:BinaryOp ,
                           hpc:RelationalOp .


hpc:LessOrEqualOp rdf:type owl:Class ;
                  rdfs:subClassOf hpc:BinaryOp ,
                                  hpc:RelationalOp .

hpc:MemberRef rdf:type owl:Class ;
              rdfs:subClassOf hpc:PostfixOp .



hpc:DefaultOptionStatement rdf:type owl:Class ;
                           rdfs:subClassOf hpc:Statement .


hpc:DerivedType rdf:type owl:Class ;
                rdfs:subClassOf hpc:Type .

hpc:Directive rdf:type owl:Class ;
              rdfs:subClassOf hpc:LanguageConstruct .


hpc:DivAssignOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:AssignmentOp .


hpc:DivOp rdf:type owl:Class ;
          rdfs:subClassOf hpc:BinaryOp .


hpc:DoWhileStatement rdf:type owl:Class ;
                     rdfs:subClassOf hpc:IterationStatement .


hpc:DotExp rdf:type owl:Class ;
           rdfs:subClassOf hpc:MemberRef .


hpc:DoubleType rdf:type owl:Class ;
               rdfs:subClassOf hpc:FloatType .


hpc:EnumDecl rdf:type owl:Class ;
             rdfs:subClassOf hpc:Declaration .


hpc:EnumType rdf:type owl:Class ;
             rdfs:subClassOf hpc:BasicType .


hpc:EnumerationConst rdf:type owl:Class ;
                     rdfs:subClassOf hpc:Constant .


hpc:EqualOp rdf:type owl:Class ;
            rdfs:subClassOf hpc:BinaryOp ,
                            hpc:EqualityOp .


hpc:EqualityOp rdf:type owl:Class ;
               rdfs:subClassOf hpc:Expression .


hpc:Expression rdf:type owl:Class ;
               rdfs:subClassOf hpc:LanguageConstruct .


hpc:ExpressionList rdf:type owl:Class ;
                   rdfs:subClassOf hpc:Unsorted .


hpc:ExpressionStatement rdf:type owl:Class ;
                        rdfs:subClassOf hpc:Statement .


hpc:Extern rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( hpc:extern
                                           )
                               ] ;
           rdfs:subClassOf hpc:StorageClass .


hpc:ExternalDataDefinition rdf:type owl:Class ;
                           rdfs:subClassOf hpc:ExternalDefinition .


hpc:ExternalDefinition rdf:type owl:Class ;
                       rdfs:subClassOf hpc:LanguageConstruct .


hpc:ExternalFunctionDefinition rdf:type owl:Class ;
                               rdfs:subClassOf hpc:ExternalDefinition ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty hpc:hasReturnType ;
                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onClass hpc:Type
                                               ] .


hpc:FloatType rdf:type owl:Class ;
              rdfs:subClassOf hpc:BasicType .


hpc:FloatingConst rdf:type owl:Class ;
                  rdfs:subClassOf hpc:Constant .


hpc:ForInitStatement rdf:type owl:Class ;
                     rdfs:subClassOf hpc:Statement .


hpc:ForStatement rdf:type owl:Class ;
                 rdfs:subClassOf hpc:IterationStatement .



hpc:Function rdf:type owl:Class ;
             owl:equivalentClass hpc:Kernel ;
             rdfs:subClassOf hpc:Statement ;
             rdfs:comment "A function of any kinds, including defining function declarations, non-defining function declarations, etc." .


hpc:FunctionCall rdf:type owl:Class ;
                 rdfs:subClassOf hpc:PostfixOp ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hpc:calls ;
                                   owl:someValuesFrom hpc:Expression
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hpc:hasArgumentExpr ;
                                   owl:someValuesFrom hpc:Expression
                                 ] ;
                 rdfs:comment "A function call site, the same as the function call expression in AST" .


hpc:FunctionDeclaration rdf:type owl:Class ;
                        rdfs:subClassOf hpc:Declaration ,
                                        hpc:Function ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty hpc:hasDefinition ;
                                          owl:someValuesFrom hpc:FunctionDefinitionBody
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty hpc:hasParameter ;
                                          owl:someValuesFrom hpc:Variable
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty hpc:hasScope ;
                                          owl:someValuesFrom hpc:Scope
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty hpc:hasType ;
                                          owl:someValuesFrom hpc:Type
                                        ] ;
                        rdfs:comment "A function declaration can be either a forward declaration or a defining declaration. If it is a defining declaration then it contains a pointer to the function definition." .


hpc:FunctionDefinitionBody rdf:type owl:Class ;
                           rdfs:subClassOf hpc:CompoundStatement ,
                                           hpc:Scope .


hpc:FunctionParameterList rdf:type owl:Class ;
                          rdfs:subClassOf hpc:Declaration .


hpc:FunctionRef rdf:type owl:Class ;
                rdfs:subClassOf hpc:PrimaryExpr ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty hpc:referTo ;
                                  owl:someValuesFrom hpc:FunctionDeclaration
                                ] .


hpc:FunctionType rdf:type owl:Class ;
                 rdfs:subClassOf hpc:DerivedType .



hpc:Global rdf:type owl:Class ;
           rdfs:subClassOf hpc:ScopeStatement .


hpc:GotoStatement rdf:type owl:Class ;
                  rdfs:subClassOf hpc:JumpStatement .


hpc:GreaterOp rdf:type owl:Class ;
              rdfs:subClassOf hpc:BinaryOp ,
                              hpc:RelationalOp .


hpc:GreaterOrEqualOp rdf:type owl:Class ;
                     rdfs:subClassOf hpc:BinaryOp ,
                                     hpc:RelationalOp .


hpc:ListExp rdf:type owl:Class ;
            rdfs:subClassOf hpc:Unsorted ;
            rdfs:comment "?" .


hpc:Literal rdf:type owl:Class ;
            rdfs:subClassOf hpc:Expression .


hpc:LogicalANDOp rdf:type owl:Class ;
                 rdfs:subClassOf hpc:Expression .


hpc:LogicalAndOp rdf:type owl:Class ;
                 rdfs:subClassOf hpc:BinaryOp .


hpc:LogicalNegOp rdf:type owl:Class ;
                 rdfs:subClassOf hpc:UnaryOp .


hpc:LogicalOROp rdf:type owl:Class ;
                rdfs:subClassOf hpc:Expression .


hpc:LogicalOrOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:BinaryOp .
hpc:LongType rdf:type owl:Class ;
             rdfs:subClassOf hpc:IntType .


hpc:Loop rdf:type owl:Class ;
         rdfs:subClassOf hpc:Statement .


hpc:Lvalue rdf:type owl:Class ;
           rdfs:subClassOf hpc:Expression ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hpc:referTo ;
                             owl:someValuesFrom hpc:Variable
                           ] ;
           rdfs:comment "An Object is a named region of storage; an lvalue is an expression referring to an object." .
hpc:ModAssignOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:AssignmentOp .


hpc:ModOp rdf:type owl:Class ;
          rdfs:subClassOf hpc:BinaryOp .

hpc:MulAssignOp rdf:type owl:Class ;
                rdfs:subClassOf hpc:AssignmentOp .


hpc:MulOp rdf:type owl:Class ;
          rdfs:subClassOf hpc:BinaryOp .
