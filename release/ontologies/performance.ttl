@prefix : <https://hpc-fair.github.io/ontology#> .
@prefix hpc: <https://hpc-fair.github.io/ontology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://hpc-fair.github.io/ontology> rdf:type owl:Ontology ;
                                       owl:versionIRI <https://hpc-fair.github.io/ontology/1.0.0> ;
                                       dc:creator "The HPC-FAIR research team"^^xsd:string ;
                                       dc:description """The HPC Ontology describes software, hardware, and artifacts in the domain
of High-Performance Computing. It can be used to annotate training
 datasets and machine learning models used in HPC software analyses
and optimizations. The goal is to make datasets and AI models FAIR.""" ;
dc:title "Performance Ontology" ;
                                       dcterms:created "2021-09-23T13:30:00-07:00"^^xsd:dateTime ;
                                       dcterms:license "the Creative Commons Attribution 4.0"^^xsd:string ;
                                       vann:preferredNamespacePrefix "hpc" ;
                                       vann:preferredNamespaceUri hpc: ;
                                       rdfs:comment "The HPC Ontology describes software, hardware, and artifacts in the domain of High-Performance Computing. It can be used to annotate training datasets and machine learning models used in HPC software analyses and optimizations. The goal is to make datasets and AI models FAIR." ;
                                       owl:versionIRI "1.0.0" .

# Anything related to performance metrics,either CPU or GPU related

###  https://github.com/HPC-FAIR/HPC-Ontology#achievedActiveWarpsPerSM
hpc:achievedActiveWarpsPerSM rdf:type owl:DatatypeProperty .


###  https://github.com/HPC-FAIR/HPC-Ontology#achievedGPUOccupancy
hpc:achievedGPUOccupancy rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf hpc:occupancy .


###  https://github.com/HPC-FAIR/HPC-Ontology#activeCyclesOfStreamingMultiprocessor
hpc:activeCyclesOfStreamingMultiprocessor rdf:type owl:DatatypeProperty ;
                                          rdfs:subPropertyOf hpc:cycle .


###  https://github.com/HPC-FAIR/HPC-Ontology#averageExecutionTime
hpc:averageExecutionTime rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf hpc:executionTime .



###  https://github.com/HPC-FAIR/HPC-Ontology#cpuPageFault
hpc:cpuPageFault rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf hpc:pageFault .

###  https://github.com/HPC-FAIR/HPC-Ontology#cycle
hpc:cycle rdf:type owl:DatatypeProperty .


###  https://github.com/HPC-FAIR/HPC-Ontology#dataTransferSize
hpc:dataTransferSize rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:size .



###  https://github.com/HPC-FAIR/HPC-Ontology#deviceToHostTransferSize
hpc:deviceToHostTransferSize rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf hpc:dataTransferSize .


###  https://github.com/HPC-FAIR/HPC-Ontology#dramUtilizationRate
hpc:dramUtilizationRate rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf hpc:percentage .




###  https://github.com/HPC-FAIR/HPC-Ontology#executionTime
hpc:executionTime rdf:type owl:DatatypeProperty .


###  https://github.com/HPC-FAIR/HPC-Ontology#executionTimePercentage
hpc:executionTimePercentage rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf hpc:percentage .



###  https://github.com/HPC-FAIR/HPC-Ontology#gpuPageFault
hpc:gpuPageFault rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf hpc:pageFault .


###  https://github.com/HPC-FAIR/HPC-Ontology#hasRmax
hpc:hasRmax rdf:type owl:DatatypeProperty ;
            rdfs:comment "The maximum achieved performance [TFlop/s]" .


###  https://github.com/HPC-FAIR/HPC-Ontology#hasRpeak
hpc:hasRpeak rdf:type owl:DatatypeProperty ;
             rdfs:comment "The theoretical peak performance [TFlop/s]" .


###  https://github.com/HPC-FAIR/HPC-Ontology#hostToDeviceTransferSize
hpc:hostToDeviceTransferSize rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf hpc:dataTransferSize .



###  https://github.com/HPC-FAIR/HPC-Ontology#l1CacheUitlizationRate
hpc:l1CacheUitlizationRate rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf hpc:percentage .


###  https://github.com/HPC-FAIR/HPC-Ontology#l2CacheUtilizationRate
hpc:l2CacheUtilizationRate rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf hpc:percentage .


###  https://github.com/HPC-FAIR/HPC-Ontology#labeledCodeVariant
hpc:labeledCodeVariant rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf hpc:codeVariant .



###  https://github.com/HPC-FAIR/HPC-Ontology#maxExecutionTime
hpc:maxExecutionTime rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:executionTime .



###  https://github.com/HPC-FAIR/HPC-Ontology#memoryOccupancy
hpc:memoryOccupancy rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf hpc:occupancy .



###  https://github.com/HPC-FAIR/HPC-Ontology#memoryThroughputRate
hpc:memoryThroughputRate rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf hpc:percentage .


###  https://github.com/HPC-FAIR/HPC-Ontology#minExecutionTime
hpc:minExecutionTime rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hpc:executionTime .


###  https://github.com/HPC-FAIR/HPC-Ontology#occupancy
hpc:occupancy rdf:type owl:DatatypeProperty .



###  https://github.com/HPC-FAIR/HPC-Ontology#overhead
hpc:overhead rdf:type owl:DatatypeProperty .


###  https://github.com/HPC-FAIR/HPC-Ontology#pageFault
hpc:pageFault rdf:type owl:DatatypeProperty .


###  https://github.com/HPC-FAIR/HPC-Ontology#streamingProcessorUtilizationRate
hpc:streamingProcessorUtilizationRate rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf hpc:percentage .


